projection;

define behavior for ZC_OBJECT alias Object
{
  use create;
  use update;
  use delete;

  use action ActivateObject;
  use action DeactivateObject;


  use association _Subobject { create; }
}

define behavior for ZC_SUBOBJECT alias Subobject
{
  use update;
  use delete;

  use association _Object;
}